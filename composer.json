{
  "name": "ruben.civeiraiglesia/docker-dashboard",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "binary/install"
  ],
  "scripts": {
    "post-install-cmd": [
      "Civi\\Lughauth\\Bootstrap\\Install::bootstrap"
    ],
    "post-update-cmd": [
      "Civi\\Lughauth\\Bootstrap\\Install::bootstrap"
    ]
  },
  "autoload": {
    "psr-4": {
      "Civi\\Lughauth\\": "src/",
      "Civi\\LughauthBackoffice\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test/"
    }
  },
  "authors": [
    {
      "name": "Ruben civeira",
      "email": "ruben.civeira@gmail.com"
    }
  ],
  "require": {
    "chillerlan/php-cache": "^5.1",
    "guzzlehttp/guzzle": "^7.9",
    "matthiasmullie/minify": "^1.3",
    "monolog/monolog": "^3.10",
    "nimbly/capsule": "^3.0",
    "open-telemetry/api": "^1.4",
    "open-telemetry/opentelemetry-logger-monolog": "^1.1",
    "php-di/php-di": "^7.1",
    "phpmailer/phpmailer": "^6.10",
    "promphp/prometheus_client_php": "^2.14",
    "league/oauth2-google": "^4.1",
    "ramsey/uuid": "^4.9",
    "respect/validation": "^2.4",
    "robmorgan/phinx": "^0.16.10",
    "slim/slim": "^4.15",
    "slim/psr7": "^1.8",
    "symfony/intl": "^7.4",
    "symfony/rate-limiter": "^7.4",
    "symfony/translation": "^7.4",
    "symfony/event-dispatcher": "^7.4",
    "symfony/yaml": "^7.4",
    "twig/twig": "^3.22",
    "vlucas/phpdotenv": "^5.6",
    "voku/html-min": "^4.5",
    "web-token/jwt-bundle": "^4.1",
    "zircote/swagger-php": "^6.0",
    "open-telemetry/sdk": "^1.6",
    "open-telemetry/exporter-otlp": "^1.3",
    "robthree/twofactorauth": "^3.0",
    "endroid/qr-code": "^6.1",
    "symfony/cache": "^7.4",
    "dragonmantank/cron-expression": "^3.6",
    "symfony/lock": "^8.0",
    "enqueue/amqp-lib": "^0.10.27"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2",
    "vimeo/psalm": "^6.14",
    "friendsofphp/php-cs-fixer": "^3.86"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false,
      "tbachert/spi": false
    }
  }
}
