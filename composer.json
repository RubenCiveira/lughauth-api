{
  "name": "ruben.civeiraiglesia/docker-dashboard",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/install"
  ],
  "scripts": {
    "post-install-cmd": [
      "Civi\\Lughauth\\Bootstrap\\Install::bootstrap"
    ],
    "post-update-cmd": [
      "Civi\\Lughauth\\Bootstrap\\Install::bootstrap"
    ]
  },
  "autoload": {
    "psr-4": {
      "Civi\\Lughauth\\": "src/",
      "Civi\\LughauthBackoffice\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "test/"
    }
  },
  "authors": [
    {
      "name": "Ruben civeira",
      "email": "ruben.civeira@gmail.com"
    }
  ],
  "require": {
    "chillerlan/php-cache": "^5.1",
    "guzzlehttp/guzzle": "^7.9",
    "matthiasmullie/minify": "^1.3",
    "monolog/monolog": "^3.9",
    "nimbly/capsule": "^3.0",
    "open-telemetry/api": "^1.3",
    "open-telemetry/opentelemetry-logger-monolog": "^1.1",
    "php-di/php-di": "^7.0",
    "phpmailer/phpmailer": "^6.10",
    "promphp/prometheus_client_php": "^2.14",
    "league/oauth2-google": "*",
    "ramsey/uuid": "^4.8",
    "respect/validation": "^2.4",
    "robmorgan/phinx": "^0.16.9",
    "slim/slim": "^4.14",
    "slim/psr7": "^1.7",
    "symfony/intl": "^7.3",
    "symfony/rate-limiter": "^7.3",
    "symfony/translation": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/yaml": "^7.3",
    "twig/twig": "^3.21",
    "vlucas/phpdotenv": "^5.6",
    "voku/html-min": "^4.5",
    "web-token/jwt-bundle": "^4.0",
    "zircote/swagger-php": "^5.1",
    "open-telemetry/sdk": "^1.5",
    "open-telemetry/exporter-otlp": "^1.3",
    "robthree/twofactorauth": "^3.0",
    "endroid/qr-code": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2",
    "vimeo/psalm": "^6.12",
    "friendsofphp/php-cs-fixer": "^3.75"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false,
      "tbachert/spi": false
    }
  }
}
